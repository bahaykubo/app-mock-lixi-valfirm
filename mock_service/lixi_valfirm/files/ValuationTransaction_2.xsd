<?xml version="1.0" encoding="UTF-8"?>
<?xar W3C XML Schema?>
<?stylesheet format="schema" config="lixi" xsl-rootelement="ValuationTransaction"?>
<xs:schema xmlns:ps="http://www.pageseeder.com/editing/2.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="ValuationTransaction">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RevisionNumber"/>
				<xs:element ref="Identifier" maxOccurs="unbounded"/>
				<xs:element ref="Date"/>
				<xs:element ref="Time"/>
				<xs:element ref="Comment" minOccurs="0"/>
				<xs:element ref="Publisher"/>
				<xs:element ref="Audience" minOccurs="0"/>
				<xs:element ref="MessageRelatesTo" minOccurs="0"/>
				<xs:element ref="MessageContact" minOccurs="0"/>
				<xs:element ref="Message"/>
				<xs:element ref="RelatedPartySegment"/>
				<xs:element ref="AttachmentSegment" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="ProductionData" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ValuationType">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Identifier" maxOccurs="unbounded"/>
				<xs:choice>
					<xs:element ref="FullRegistered" minOccurs="0"/>
					<xs:element ref="ProgressInspection" minOccurs="0"/>
					<xs:element ref="Costing" minOccurs="0"/>
					<xs:element ref="RestrictedAccessAssessment" minOccurs="0"/>
					<xs:element ref="WorkFlow" minOccurs="0"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:annotation/>
	<xs:element name="Identifier">
		<xs:complexType>
			<xs:attribute name="UniqueID" type="xs:string" use="required"/>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="AggregatorAssigned"/>
						<xs:enumeration value="BrokerAssigned"/>
						<xs:enumeration value="CustomerAssigned"/>
						<xs:enumeration value="LenderAssigned"/>
						<xs:enumeration value="LIXIcode"/>
						<xs:enumeration value="MIAssigned"/>
						<xs:enumeration value="NomineeAssigned"/>
						<xs:enumeration value="Sequential"/>
						<xs:enumeration value="TIAssigned"/>
						<xs:enumeration value="ValuerAssigned"/>
						<xs:enumeration value="VPMAssigned"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Description" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RevisionNumber">
		<xs:complexType>
			<xs:attribute name="LIXIVersion" type="xs:string" use="required"/>
			<xs:attribute name="LenderVersion" type="xs:string" use="optional"/>
			<xs:attribute name="UserVersion" type="xs:string" use="optional"/>
			<xs:attribute name="UserType" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Lender"/>
						<xs:enumeration value="TitleInsurer"/>
						<xs:enumeration value="Valuer"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Date" type="xs:date"/>
	<xs:element name="Time" type="xs:time"/>
	<xs:element name="Publisher">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RelatedEntityRef"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Audience">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RelatedEntityRef" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RelatedEntityRef">
		<xs:complexType>
			<xs:attribute name="RelatedID" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Message">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Identifier"/>
				<xs:element ref="MessageRelatesTo"/>
				<xs:element ref="MessageContact" minOccurs="0"/>
				<xs:element ref="MessageBody" maxOccurs="unbounded"/>
				<xs:element ref="ValuationType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MessageRelatesTo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Identifier" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MessageFileName" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MessageFileName">
		<xs:complexType/>
	</xs:element>
	<xs:element name="MessageContact">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RelatedEntityRef"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MessageBody">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="MessageAnnotation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="ErrorLocation" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
				<xs:element ref="Status" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Event" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Information"/>
						<xs:enumeration value="DataError"/>
						<xs:enumeration value="SystemError"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ErrorSource">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Parser"/>
						<xs:enumeration value="DTD"/>
						<xs:enumeration value="XSD"/>
						<xs:enumeration value="XSLT"/>
						<xs:enumeration value="Schematron"/>
						<xs:enumeration value="Proprietary"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ErrorSourceVersion" type="xs:string"/>
			<xs:attribute name="Order" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="MessageAnnotation">
		<xs:complexType mixed="true">
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Code"/>
						<xs:enumeration value="Message"/>
						<xs:enumeration value="EndUserMessage"/>
						<xs:enumeration value="Technical"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ErrorLocation">
		<xs:complexType mixed="true">
			<xs:attribute name="ColNumber" type="xs:string"/>
			<xs:attribute name="LineNumber" type="xs:string"/>
			<xs:attribute name="FilePosition" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Status">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date"/>
				<xs:element ref="Time" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Name" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Initial"/>
						<xs:enumeration value="Inquiring"/>
						<xs:enumeration value="Instructed"/>
						<xs:enumeration value="Accepted"/>
						<xs:enumeration value="Assigned"/>
						<xs:enumeration value="Declined"/>
						<xs:enumeration value="InProgress"/>
						<xs:enumeration value="Inspected"/>
						<xs:enumeration value="AwaitingAuthorisation"/>
						<xs:enumeration value="ValuationCompleted"/>
						<xs:enumeration value="Finalised"/>
						<xs:enumeration value="AmendmentRequired"/>
						<xs:enumeration value="Cancelled"/>
						<xs:enumeration value="Delayed"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="StatusOwner" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Event">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date"/>
				<xs:element ref="Time" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Name" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Person">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RelatedEntityRef" minOccurs="0"/>
				<xs:element ref="PersonName"/>
				<xs:element ref="ContactDetails"/>
			</xs:sequence>
			<xs:attribute name="CompanyDirector">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FirstHomeBuyer">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PreviousName" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Company">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CompanyName"/>
				<xs:element ref="ContactDetails" maxOccurs="unbounded"/>
				<xs:element ref="WebAddress" minOccurs="0"/>
				<xs:element ref="Director" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="CompanyType"/>
				<xs:element ref="CompanyNumber"/>
				<xs:element ref="RegisteredIn"/>
			</xs:sequence>
			<xs:attribute name="Status">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Exempt"/>
						<xs:enumeration value="NonExempt"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompanyName">
		<xs:complexType mixed="true">
			<xs:attribute name="BusinessName" type="xs:string"/>
			<xs:attribute name="LicenceNumber" type="xs:string"/>
			<xs:attribute name="PublicAuthority">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompanyNumber">
		<xs:complexType>
			<xs:attribute name="ABN" type="xs:string"/>
			<xs:attribute name="ACN" type="xs:string"/>
			<xs:attribute name="ABRN" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompanyType">
		<xs:complexType>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Ltd"/>
						<xs:enumeration value="NL"/>
						<xs:enumeration value="Inc"/>
						<xs:enumeration value="PtyLtd"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="NumberOfDirectors" type="xs:string"/>
			<xs:attribute name="Description">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="SoleTrader"/>
						<xs:enumeration value="Partnership"/>
						<xs:enumeration value="Corporation"/>
						<xs:enumeration value="Trust"/>
						<xs:enumeration value="NonProfitOrganisation"/>
						<xs:enumeration value="FederalPublicEnterprise"/>
						<xs:enumeration value="StatePublicEnterprise"/>
						<xs:enumeration value="LocalPublicEnterprise"/>
						<xs:enumeration value="Superannuation"/>
						<xs:enumeration value="FederalGovernment"/>
						<xs:enumeration value="StateGovernment"/>
						<xs:enumeration value="LocalGovernment"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="OtherDescription" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RegisteredIn">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date"/>
				<xs:sequence>
					<xs:element ref="Country"/>
					<xs:element ref="State"/>
				</xs:sequence>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactDetails">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="RelatedEntityRef" maxOccurs="unbounded"/>
				<xs:sequence>
					<xs:element ref="AddressDetails" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="HomePhone" minOccurs="0"/>
					<xs:element ref="WorkPhone" minOccurs="0"/>
					<xs:element ref="Email" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Director">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RelatedEntityRef"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PersonName">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NameTitle" minOccurs="0"/>
				<xs:element ref="FirstName" minOccurs="0"/>
				<xs:element ref="OtherName" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AlsoKnownAs" minOccurs="0"/>
				<xs:element ref="Surname"/>
			</xs:sequence>
			<xs:attribute name="LanguageSpoken" type="xs:string"/>
			<xs:attribute name="IsPreferredContact">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="MostPreferred"/>
						<xs:enumeration value="SecondPreferred"/>
						<xs:enumeration value="ThirdPreferred"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="NameTitle">
		<xs:complexType>
			<xs:attribute name="Value">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Mr"/>
						<xs:enumeration value="Mrs"/>
						<xs:enumeration value="Miss"/>
						<xs:enumeration value="Ms"/>
						<xs:enumeration value="Dr"/>
						<xs:enumeration value="Prof"/>
						<xs:enumeration value="Rev"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Description" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FirstName" type="xs:string"/>
	<xs:element name="OtherName" type="xs:string"/>
	<xs:element name="AlsoKnownAs" type="xs:string"/>
	<xs:element name="Surname" type="xs:string"/>
	<xs:element name="RealEstate">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Identifier" maxOccurs="unbounded"/>
				<xs:element ref="Residential"/>
				<xs:element ref="EstimatedValue" minOccurs="0"/>
				<xs:element ref="Location" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Status">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="NewBuilding"/>
						<xs:enumeration value="ToBeBuilt"/>
						<xs:enumeration value="Established"/>
						<xs:enumeration value="VacantLand"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PrimarySecurity">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="InvestmentPropertyLetter">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ApprovalInPrinciple">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SecureOther">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Construction">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FloorArea" type="xs:string"/>
			<xs:attribute name="NumberOfBedrooms" type="xs:string"/>
			<xs:attribute name="IsTrustAsset">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="LVRPercentage" type="xs:string"/>
			<xs:attribute name="MortgageInsurance">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Occupancy">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="OwnerPrimary"/>
						<xs:enumeration value="OwnerSecondary"/>
						<xs:enumeration value="NonOwner"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Prepurchase">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SDTransfer" type="xs:string"/>
			<xs:attribute name="SelfConveyance">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="LandAreaHectares" type="xs:string"/>
			<xs:attribute name="TOTransfer" type="xs:string"/>
			<xs:attribute name="TOTotal" type="xs:string"/>
			<xs:attribute name="TOCompanyTitle" type="xs:string"/>
			<xs:attribute name="TOOther" type="xs:string"/>
			<xs:attribute name="TOOtherDescription" type="xs:string"/>
			<xs:attribute name="Transaction">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Owns"/>
						<xs:enumeration value="Purchasing"/>
						<xs:enumeration value="Refinancing"/>
						<xs:enumeration value="Selling"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Holding">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Sole"/>
						<xs:enumeration value="JointTenants"/>
						<xs:enumeration value="TennantCommonInSharesOf"/>
						<xs:enumeration value="TennantCommonInEqualShares"/>
						<xs:enumeration value="CompanyShare"/>
						<xs:enumeration value="TimeShare"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ValuationProgram">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="InPrinciple"/>
						<xs:enumeration value="NoValuation"/>
						<xs:enumeration value="Regular"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="AcceptableSecurity">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ZoningAcceptable">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="PropertyPrimaryPurpose">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="OwnerOccupied"/>
						<xs:enumeration value="Investment"/>
						<xs:enumeration value="Business"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Zoning" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Residential">
		<xs:complexType>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="AptUnitFlat"/>
						<xs:enumeration value="SemiDetachedHouse"/>
						<xs:enumeration value="FullyDetachedHouse"/>
						<xs:enumeration value="NewStrataTitleUnit"/>
						<xs:enumeration value="StrataTitleUnit"/>
						<xs:enumeration value="CompanyTitleUnit"/>
						<xs:enumeration value="ConvertedCommercialProperty"/>
						<xs:enumeration value="ConvertedMotelUnits"/>
						<xs:enumeration value="ConvertedIndustrialProperty"/>
						<xs:enumeration value="HolidayHome"/>
						<xs:enumeration value="HolidayRental"/>
						<xs:enumeration value="Terrace"/>
						<xs:enumeration value="Townhouse"/>
						<xs:enumeration value="Duplex"/>
						<xs:enumeration value="Timeshare"/>
						<xs:enumeration value="RentalGuarantee"/>
						<xs:enumeration value="GovtRentalGuarantee"/>
						<xs:enumeration value="OwnerBuilderHouseConstruction"/>
						<xs:enumeration value="LicencedBuilderHouseConstruction"/>
						<xs:enumeration value="ResortUnit"/>
						<xs:enumeration value="ServicedApt"/>
						<xs:enumeration value="Villa"/>
						<xs:enumeration value="VacantLand"/>
						<xs:enumeration value="PropertyDevelopment"/>
						<xs:enumeration value="StudioWarehouseApt"/>
						<xs:enumeration value="BedsitterBachelor"/>
						<xs:enumeration value="SingleBedroomLess50m2"/>
						<xs:enumeration value="LuxuryHouse"/>
						<xs:enumeration value="LuxuryOther"/>
						<xs:enumeration value="UnitStudentAccom"/>
						<xs:enumeration value="HomeUnit"/>
						<xs:enumeration value="MotelHotelUnit"/>
						<xs:enumeration value="StudioUnit"/>
						<xs:enumeration value="AgedCareUnit"/>
						<xs:enumeration value="RuralResidentialWithDwelling"/>
						<xs:enumeration value="VacantRuralResidentialSite"/>
						<xs:enumeration value="Other"/>

	<xs:element name="RiskAnalysis">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RiskRating" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="EnvironmentIssues" type="xs:string"/>
			<xs:attribute name="HeritageIssue" type="xs:string"/>
			<xs:attribute name="Marketability" type="xs:string"/>
			<xs:attribute name="OverallRiskAnalysis" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RiskRating">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Identifier"/>
				<xs:element ref="Comment"/>
			</xs:sequence>
			<xs:attribute name="RatingType" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="LocationNeighbourhood"/>
						<xs:enumeration value="Land"/>
						<xs:enumeration value="Improvements"/>
						<xs:enumeration value="Environmental"/>
						<xs:enumeration value="MarketSegment"/>
					  <xs:enumeration value="ReducedValue"/>
						<xs:enumeration value="RecentMarket"/>
						<xs:enumeration value="LocalEconomy"/>
						<xs:enumeration value="MarketVolatility"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Rating" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="0-NotKnown"/>
						<xs:enumeration value="1-Low"/>
						<xs:enumeration value="2-LowToMedium"/>
						<xs:enumeration value="3-Medium"/>
						<xs:enumeration value="4-MediumToHigh"/>
						<xs:enumeration value="5-High"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ValueComponent">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MarketValueAsIfComplete" minOccurs="0"/>
				<xs:element ref="EstimatedSettlement"/>
				<xs:element ref="Comment"/>
			</xs:sequence>
			<xs:attribute name="LandValue" use="required"/>
			<xs:attribute name="ImprovementsValue"/>
			<xs:attribute name="OtherValue"/>
			<xs:attribute name="OtherValueDescription" type="xs:string"/>
			<xs:attribute name="LikelyWeeklyUnfRental"/>
			<xs:attribute name="InsuranceAssessment"/>
			<xs:attribute name="RequiresRecommendation">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="RecommendedSecurity">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="RecommendationReason" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="EstimatedSettlement">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="Date"/>
				<xs:element ref="Duration"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Zoning">
		<xs:complexType>
			<xs:attribute name="ZoningType">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Residential"/>
						<xs:enumeration value="Commercial"/>
						<xs:enumeration value="CommercialResidential"/>
						<xs:enumeration value="Rural"/>
						<xs:enumeration value="RuralResidential"/>
						<xs:enumeration value="ShortStayAccommodation"/>
						<xs:enumeration value="Urban"/>
						<xs:enumeration value="Other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="OtherDescription" type="xs:string"/>
			<xs:attribute name="ZoningCategory" type="xs:string"/>
			<xs:attribute name="LocalGovtAuthority" type="xs:string"/>
			<xs:attribute name="ZoningEffect" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="SiteDetailResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Identifier" minOccurs="0"/>
				<xs:element ref="SiteDimensions" minOccurs="0"/>
				<xs:element ref="SiteArea"/>
				<xs:element ref="Locality" minOccurs="0"/>
				<xs:element ref="Neighbourhood" minOccurs="0"/>
				<xs:element ref="SiteDescription" minOccurs="0"/>
				<xs:element ref="DetailedComment" minOccurs="0"/>
				<xs:element ref="Comment" minOccurs="0"/>
				<xs:element ref="PropertyCharacteristics" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="TitleSighted">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="TitleSearched">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SiteDescription">
		<xs:complexType mixed="true">
			<xs:attribute name="CurrentUse" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Residential"/>
						<xs:enumeration value="Non-Residential"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ElevationPerStreetLevel" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Above"/>
						<xs:enumeration value="Below"/>
						<xs:enumeration value="Level"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SiteDimensions">
		<xs:complexType>
			<xs:attribute name="Frontage" use="required"/>
			<xs:attribute name="Depth"/>
			<xs:attribute name="UnitOfMeasurement" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="LinearMetres"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="IrregularShape" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Description" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="UnitDetails">
		<xs:complexType>
			<xs:attribute name="TotalUnitsInDevelopment" use="required"/>
			<xs:attribute name="SubjectUnitEntitlement" use="required"/>
			<xs:attribute name="TotalUnitEntitlement" use="required"/>
			<xs:attribute name="NumLevelsInDevelopment" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="AncillaryFeature" type="xs:string"/>
	<xs:element name="SalesEvidence">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Location"/>
				<xs:element ref="Date"/>
				<xs:choice>
					<xs:element ref="DetailedComment" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="Comment" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="SalePrice" type="xs:string" use="required"/>
			<xs:attribute name="PropertySaleType">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="ThisProperty"/>
						<xs:enumeration value="SimilarProperty"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="OverallComparability">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Superior"/>
						<xs:enumeration value="Inferior"/>
						<xs:enumeration value="Comparable"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="LandComparability">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Superior"/>
						<xs:enumeration value="Inferior"/>
						<xs:enumeration value="Comparable"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ImprovementsComparability">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Superior"/>
						<xs:enumeration value="Inferior"/>
						<xs:enumeration value="Comparable"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="LocationComparability">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Superior"/>
						<xs:enumeration value="Inferior"/>
						<xs:enumeration value="Comparable"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ConditionComparability">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Superior"/>
						<xs:enumeration value="Inferior"/>
						<xs:enumeration value="Comparable"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SaleEvidenceSource">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Agent"/>
						<xs:enumeration value="OtherValuer"/>
						<xs:enumeration value="Contractor"/>
						<xs:enumeration value="StatutoryRecords"/>
						<xs:enumeration value="CommercialProvider"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="OtherSimilarity" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompletionDetails">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ValuationValidFrom"/>
				<xs:element ref="DateOfInspection"/>
				<xs:element ref="Locality" minOccurs="0"/>
				<xs:element ref="Neighbourhood" minOccurs="0"/>
				<xs:element ref="MarketValueAsIfComplete" minOccurs="0"/>
				<xs:element ref="CostingComments" minOccurs="0"/>
				<xs:element ref="Comment" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="LendersCaution">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="LendersCautionComment" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ValuationValidFrom">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DateOfInspection">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date"/>
				<xs:element ref="Time" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Locality">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Amenity" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Amenity">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Identifier"/>
				<xs:element ref="Description"/>
				<xs:element ref="DistanceFromProperty"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DistanceFromProperty">
		<xs:complexType>
			<xs:attribute name="DistanceInKlms" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
						<xs:enumeration value="4"/>
						<xs:enumeration value="5"/>
						<xs:enumeration value="6"/>
						<xs:enumeration value="7"/>
						<xs:enumeration value="8"/>
						<xs:enumeration value="9"/>
						<xs:enumeration value="10"/>
						<xs:enumeration value="11-20"/>
						<xs:enumeration value="21-30"/>
						<xs:enumeration value="31-40"/>
						<xs:enumeration value="41-50"/>
						<xs:enumeration value="Over50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Neighbourhood" type="xs:string"/>
	<xs:element name="MarketValueAsIfComplete">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Description"/>
			</xs:sequence>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Range"/>
						<xs:enumeration value="SinglePoint"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ValueFloor"/>
			<xs:attribute name="ValueCeiling" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CostingComments">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BuildersTender" minOccurs="0"/>
				<xs:element ref="ValuersCostEstimate" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="ProposedDevelopment" type="xs:string"/>
			<xs:attribute name="DocumentsCosted" type="xs:string"/>
			<xs:attribute name="ConstructionComments" type="xs:string"/>
			<xs:attribute name="CostingStatus" type="xs:string"/>
			<xs:attribute name="ProjectViability" type="xs:string"/>
			<xs:attribute name="DurationStatus" type="xs:string"/>
			<xs:attribute name="ContractAgreement" type="xs:string"/>
			<xs:attribute name="BuilderAndCustomerDocStatus" type="xs:string"/>
			<xs:attribute name="ConstructionFeasibility" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BuildersTender">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RelatedEntityRef" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="TenderAmount" type="xs:string" use="required"/>
			<xs:attribute name="GSTAmount" type="xs:string" use="required"/>
			<xs:attribute name="FreightAmount" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ValuersCostEstimate">
		<xs:complexType>
			<xs:attribute name="ConstructionAmount" type="xs:string" use="required"/>
			<xs:attribute name="GSTAmount" type="xs:string" use="required"/>
			<xs:attribute name="FreightAmount" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ProgressCompletionDetails">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ValuationValidFrom"/>
				<xs:element ref="DateOfInspection"/>
				<xs:element ref="Comment" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="CostToComplete" type="xs:string" use="required"/>
			<xs:attribute name="CostToDate" type="xs:string" use="required"/>
			<xs:attribute name="RecommendedProgressPayment" type="xs:string" use="required"/>
			<xs:attribute name="LendersCautionComment" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Disclaimer" type="xs:string"/>
	<xs:element name="SalesMarket">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RecentSale" minOccurs="0"/>
				<xs:element ref="EarlierSale" minOccurs="0"/>
				<xs:element name="ExpectedSellingPeriodComment" minOccurs="0"/>
				<xs:element name="ContractSightedComment" minOccurs="0"/>
				<xs:element ref="Comment" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="MarketActivity" type="xs:string" use="optional"/>
		  <xs:attribute name='MultiTierMarket'>
		    <xs:simpleType>
		      <xs:restriction base='xs:NMTOKENS'>
		        <xs:enumeration value='No' />
		        <xs:enumeration value='Yes' />
		      </xs:restriction>
		    </xs:simpleType>
		  </xs:attribute>
		  <xs:attribute name='MultiTierMarketComment' type='xs:string' />
			<xs:attribute name="SaleInlineWithLocalMarket">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKENS">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="SaleInlineWithLocalMarketComment" type="xs:string"/>
			<xs:attribute name="ExpectedSellingPeriod">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKENS">
						<xs:enumeration value="Less than 6 months"/>
						<xs:enumeration value="6 to 12 months"/>
						<xs:enumeration value="Greater than 12 months"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ContractSighted">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKENS">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="No"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="EarlierSale">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date"/>
				<xs:element ref="Comment"/>
			</xs:sequence>
			<xs:attribute name="SalePrice" type="xs:decimal"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="RecentSale">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Date" minOccurs="0"/>
				<xs:element ref="Comment"/>
			</xs:sequence>
			<xs:attribute name="SalePrice" type="xs:decimal"/>
			<xs:attribute name="IsContract">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKENS">
						<xs:enumeration value="No"/>
						<xs:enumeration value="Yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ImportantNotesAndQualifications" type="xs:string"/>
</xs:schema>
